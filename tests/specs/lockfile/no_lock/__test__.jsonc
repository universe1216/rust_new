{
  "tests": {
    "info": {
      "args": "info main.ts",
      "output": "fail.out",
      "exitCode": 10
    },
    "info_no_lock": {
      "args": "info --no-lock main.ts",
      "output": "info.nolock.out"
    },
    "bench": {
      "args": "bench",
      "output": "fail.out",
      "exitCode": 10
    },
    "bench_no_lock": {
      "args": "bench --no-lock",
      "output": "bench.nolock.out"
    },
    "doc": {
      "args": "doc main.ts",
      "exitCode": 10,
      "output": "fail.out"
    },
    "doc_no_lock": {
      "args": "doc --no-lock main.ts",
      "output": "doc.nolock.out"
    },
    "test": {
      "args": "test",
      "exitCode": 10,
      "output": "fail.out"
    },
    "test_no_lock": {
      "args": "test --no-lock",
      "output": "test.nolock.out"
    }
  }
}
